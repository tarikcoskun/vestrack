{
  "version": "0.2.0",
  "configurations": [
    {
      "command": "pnpm dev",
      "name": "Development server",
      "request": "launch",
      "type": "node-terminal",
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "Local:        http://localhost:([0-9]+)",
        "uriFormat": "http://localhost:%s"
      }
    }
  ]
}
